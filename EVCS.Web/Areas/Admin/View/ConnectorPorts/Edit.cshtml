@model EVCS.Services.DTOs.ConnectorPortUpsertDto
@{
    ViewData["Title"] = "Edit Port";
}
<h2>Edit Port</h2>
<form asp-action="Edit" method="post" class="mt-3">
    <input type="hidden" asp-for="Id" />
    <partial name="_Form" />
    <div class="mt-3 d-flex gap-2">
        <button class="btn btn-primary">Update</button>
        <a asp-action="Index" asp-route-chargerId="@Model.ChargerId" class="btn btn-outline-secondary">Back</a>
    </div>
</form>