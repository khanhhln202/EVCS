@model ConnectorPortUpsertDto
@{
    ViewData["Title"] = "Edit Port";
}
<h2 class="mb-3"><i class="bi bi-pencil-square"></i> Edit Port</h2>
<partial name="_ValidationSummary" />
<form asp-action="Edit" method="post" class="mt-3">
    <input type="hidden" asp-for="Id" />
    <partial name="_Form" />
    <div class="mt-3 d-flex gap-2">
        <button class="btn btn-primary"><i class="bi bi-save"></i> Update</button>
        <a asp-action="Index" asp-route-chargerId="@Model.ChargerId" class="btn btn-outline-secondary">Back</a>
    </div>
</form>
@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}