@model ConnectorPortUpsertDto
<input type="hidden" asp-for="ChargerId" />
<div class="row g-3">
    <div class="col-md-2">
        <div class="form-floating">
            <input asp-for="IndexNo" class="form-control" placeholder="#" />
            <label asp-for="IndexNo"></label>
        </div>
    </div>
    <div class="col-md-3">
        <div class="form-floating">
            <select asp-for="ConnectorType" class="form-select">
                <option value="">(None)</option>
                <option>Type2</option>
                <option>CCS2</option>
                <option>CHAdeMO</option>
            </select>
            <label asp-for="ConnectorType"></label>
        </div>
    </div>
    <div class="col-md-3">
        <div class="form-floating">
            <input asp-for="MaxPowerKw" class="form-control" placeholder="0" />
            <label asp-for="MaxPowerKw"></label>
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-floating">
            <input asp-for="DefaultPricePerKwh" class="form-control" placeholder="0" />
            <label asp-for="DefaultPricePerKwh"></label>
        </div>
    </div>
    <div class="col-md-3">
        <div class="form-floating">
            <select asp-for="Status" asp-items="Html.GetEnumSelectList<EVCS.Models.Enums.ConnectorPortStatus>()" class="form-select"></select>
            <label asp-for="Status"></label>
        </div>
    </div>
    <input type="hidden" asp-for="RowVersion" />
</div>