@model EVCS.Services.DTOs.ChargerUnitUpsertDto
@{
    ViewData["Title"] = "Create Charger";
}
<h2>Create Charger</h2>
<form asp-action="Create" method="post" class="mt-3">
    <partial name="_Form" />
    <div class="mt-3 d-flex gap-2">
        <button class="btn btn-primary">Save</button>
        <a asp-action="Index" asp-route-stationId="@Model.StationId" class="btn btn-outline-secondary">Cancel</a>
    </div>
</form>